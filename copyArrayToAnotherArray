class Main {
    public static void main(String[] args) {
        int[] a1=new int[]{3,2,16,4,4};
        int siz=a1.length;
       // System.out.println(siz);
        int[] a2=new int[siz];
        for(int i=0;i<siz;i++){
            a2[i]=a1[i];
            
        }
        for( int i:a2){
            System.out.print(i+" ");
        }
        
    }
}
